{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:admin": "cd admin && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:admin\"",
    "build": "vite build",
    "build:admin": "cd admin && npm run build",
    "build:all": "npm run build && npm run build:admin",
    "build:prod": "NODE_ENV=production tsc -b && vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:fast": "vite build --config vite.config.fast.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "deploy:firebase": "./scripts/deploy-firebase.sh",
    "deploy:gcloud": "./scripts/deploy-gcloud.sh",
    "deploy:main": "npm run build && firebase deploy --only hosting:main",
    "deploy:admin": "npm run build:admin && firebase deploy --only hosting:admin",
    "deploy:all": "npm run build:all && firebase deploy",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "deploy:indexes": "firebase deploy --only firestore:indexes",
    "deploy:complete": "npm run build:all && firebase deploy --only hosting,firestore:rules,firestore:indexes",
    "typecheck": "tsc --noEmit",
    "install:all": "npm install && cd admin && npm install",
    "clean": "rm -rf dist admin/dist node_modules admin/node_modules",
    "clean:build": "rm -rf dist admin/dist",
    "fresh-install": "npm run clean && npm run install:all",
    "seed:whale-watching": "cd scripts && npx ts-node seedWhaleWatchingData.ts",
    "seed:pilgrimage-tours": "cd scripts && npx ts-node seedPilgrimageToursData.ts",
    "seed:island-getaways": "cd scripts && npx ts-node seedIslandGetawaysData.ts",
    "seed:hikkaduwa-water-sports": "cd scripts && npx ts-node seedHikkaduwaWaterSportsData.ts",
    "seed:hot-air-balloon-sigiriya": "cd scripts && npx ts-node seedHotAirBalloonSigiriyaData.ts",
    "seed:kalpitiya-kitesurfing": "cd scripts && npx ts-node seedKalpitiyaKitesurfingData.ts",
    "seed:luxury-experiences": "cd scripts && npx ts-node seedLuxuryExperiences.ts",
    "seed:jungle-camping": "cd scripts && npx ts-node seedJungleCampingData.ts",
    "seed:lagoon-safari": "cd scripts && npx ts-node seedLagoonSafariData.ts",
    "seed:cooking-class": "cd scripts && npx ts-node seedCookingClassData.ts",
    "seed:tea-trails": "cd scripts && npx ts-node seedTeaTrails.ts"
